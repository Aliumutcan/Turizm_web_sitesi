        <script>
            var sayac = 0;
            $(document).ready(function () {
                resimler();
                setInterval(function () {
                    resimler()

                }, 5000);
            })

            function resimler() {
                var galeriboyutu = document.getElementById("galeriboyutu").getAttribute("value");

                if (sayac == galeriboyutu) {
                    sayac = 0;
                }
                var src = document.getElementById("bg_" + sayac).getAttribute("src");
                var link = document.getElementById("bg_" + sayac).getAttribute("data-target");
                document.getElementById("banner").style.backgroundImage = "url('" + src + "')";
                document.getElementById("bannerlink").setAttribute("href", link);
                sayac++;
            }
        </script>
